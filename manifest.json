{
  "manifest_version": 3,
  "name": "goTab",
  "version": "1.0",
  "description": "Navigate to URLs using custom keywords",
  "author": {
    "name": "James Stevens",
    "email": "jstevens_dev@proton.me"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "omnibox": {
    "keyword": "go"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}